    character: {fileID: 0}
    sound: {fileID: 0}
    name: 
    isHighlight: 0
    text: "\u3053\u3053\u3067\u904E\u3054\u3059\u4E00\u65E5\u306B\u8A71\u3059\u3053\u3068\u306A\u3069\u306A\u3044\u3057\u3001\u77E5\u80FD\u691C\u67FB\u3068\u3084\u3089\u306E\u5185\u5BB9\u3082\u3075\u3056\u3051\u3066\u3044\u308B\u3068\u3057\u304B\u601D\u3048\u306A\u3044\u3002\r\n\u8D64\u5B50\u304B\u3001\u305D\u308C\u3068\u3082\u5E74\u8001\u3044\u305F\u8001\u4EBA\u3060\u3068\u3067\u3082\u601D\u308F\u308C\u3066\u3044\u308B\u306E\u304B\u3002"
