    sound: {fileID: 0}
    name: "\u30CF\u30B8\u30E1"
    isHighlight: 0
    canChange: 1
    text: "\u3046\u3093\u3002\u5B9F\u306F\u306D\u3002\n\u50D5\u306F\u3044\u3064\u304B\u5916\u306E\u4E16\u754C\u3092\u65C5\u3057\u3066\u3001\u8272\u3093\u306A\u4EBA\u3068\u8A71\u3057\u3066\u307F\u305F\u3044\u3093\u3060\u3002"
  - background: {fileID: 21300000, guid: 22d298df322387e448f25d3331bba48b, type: 3}
