    sound: {fileID: 0}
    name: 
    isHighlight: 0
    canChange: 1
    text: "\u2500\u2500\u2500\u3067\u3082\u3001\u5B50\u4F9B\u3089\u3057\u3044\u5922\u3060\u308D\u3046\u3002"
