    sound: {fileID: 0}
    name: "\u30E6\u30AD"
    isHighlight: 1
    canChange: 1
    text: "\u3046\u3046\u3093\u3002\u3059\u3054\u304F\u3001\u7D20\u6575\u306A\u5922\u3060\u3068\u601D\u3046\u3002"
