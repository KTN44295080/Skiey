    sound: {fileID: 0}
    name: "\u30CF\u30B8\u30E1"
    isHighlight: 0
    canChange: 1
    text: "\u3053\u3053\u306F\u7A7A\u6C17\u3082\u3001\u307F\u3093\u306A\u306E\u5FC3\u3082\u3002\u6697\u304F\u91CD\u305F\u304F\u3066\u606F\u304C\u8A70\u307E\u308B\u3002\n\u3060\u304B\u3089\u3044\u3064\u304B\u3053\u3053\u3092\u51FA\u308B\u3002\n\u2026\u3082\u3057\u51FA\u3089\u308C\u308B\u6642\u304C\u6765\u306A\u3044\u306E\u306A\u3089\u8131\u8D70\u3059\u308B\u3002"
  - background: {fileID: 21300000, guid: 22d298df322387e448f25d3331bba48b, type: 3}
