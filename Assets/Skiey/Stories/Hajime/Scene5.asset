    sound: {fileID: 0}
    name: 
    isHighlight: 0
    canChange: 1
    text: "\u6C17\u5727\u3055\u308C\u3066\u5C11\u3057\u5F15\u304D\u6C17\u5473\u306B\u8FD4\u4E8B\u3092\u3059\u308B\u3002"
