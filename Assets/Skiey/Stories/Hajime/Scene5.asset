    name: "\u30E6\u30AD"
    isHighlight: 1
    canChange: 1
    text: "\u2026\u3042\u306A\u305F\u3082\u5916\u306E\u4E16\u754C\u306B\u8208\u5473\u304C\u3042\u308B\u306E\uFF1F"
