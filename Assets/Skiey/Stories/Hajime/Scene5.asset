    sound: {fileID: 0}
    name: 
    isHighlight: 0
    canChange: 1
    text: "\u5F7C\u5973\u306F\u3001\u5446\u6C17\u306B\u3068\u3089\u308C\u305F\u306E\u304B\u3002\n\u6570\u79D2\u56FA\u307E\u3063\u305F\u3042\u3068\u3002"
